{
  "name": "alphavantage",
  "display_name": "Alphavantage MCP Server",
  "version": "1.0.0",
  "description": "MCP server for stock market data API [AlphaVantage](https://www.alphavantage.co/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/calvernaz/alphavantage"
  },
  "homepage": "https://github.com/calvernaz/alphavantage",
  "author": {
    "name": "calvernaz"
  },
  "license": "Apache-2.0",
  "categories": [
    "API",
    "Stock Market"
  ],
  "tags": [
    "alphavantage",
    "stock market"
  ],
  "installations": {
    "uvx":{
      "type": "uvx",
      "command": "uvx",
      "args": [
          "--directory",
          "<DIRECTORY-OF-CLONED-PROJECT>/alphavantage",
          "run",
          "alphavantage"
        ],
        "env": {
          "ALPHAVANTAGE_API_KEY": "YOUR_API_KEY_HERE"
        }
      }
    }
}